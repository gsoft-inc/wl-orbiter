{
  "name": "sg-orbiter",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/gsoft-inc/wl-orbiter.git",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "pnpm run start:pkg & pnpm run start:sb",
    "start:pkg": "pnpm -r --parallel run start",
    "start:sb": "storybook dev -p 9009",
    "start-sb-chroma": "cross-env STORYBOOK_IS_CHROMATIC=true pnpm start:sb",
    "start-sb-docs": "cross-env STORYBOOK_IS_DOCS_SITE=true storybook dev -p 9010 --docs",
    "start-sb-no-chroma": "cross-env STORYBOOK_INCLUDE_CHROMATIC=false pnpm start:sb",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=16384 pnpm -r build",
    "build-storybook": "pnpm build-sb-chroma",
    "build-sb": "cross-env NODE_OPTIONS=--max-old-space-size=16384 storybook build -o storybook-static/sb",
    "build-sb-chroma": "cross-env STORYBOOK_IS_CHROMATIC=true NODE_OPTIONS=--max-old-space-size=16384 storybook build -o storybook-static/chromatic",
    "build-sb-docs": "cross-env STORYBOOK_IS_DOCS_SITE=true NODE_OPTIONS=--max-old-space-size=16384 storybook build -o storybook-static/docs --docs",
    "build-netlify-sb": "pnpm build && pnpm build-sb",
    "build-netlify-docs": "pnpm build && pnpm build-sb-docs",
    "clean": "pnpm run \"/^clean:.*/\"",
    "clean:pkg": "pnpm run --parallel -r clean",
    "clean:sb": "rimraf storybook-static",
    "reset": "pnpm clean && pnpm dlx rimraf node_modules pnpm-lock.yaml",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:eslint": "eslint --ext .js,.jsx,.ts,.tsx,.mdx . --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:style": "stylelint \"**/*.css\" --allow-empty-input --cache",
    "lint:typecheck": "pnpm -r --parallel --include-workspace-root exec tsc --noEmit",
    "lint-ci-eslint": "eslint --ext .js,.jsx,.ts,.tsx,.mdx . --max-warnings=0",
    "lint-ci-style": "stylelint \"**/*.css\" --allow-empty-input",
    "lint-ci-typecheck": "pnpm -r --parallel --include-workspace-root exec tsc --noEmit",
    "list-outdated-deps": "pnpm outdated -r --format list",
    "update-outdated-deps": "pnpm update -r --latest \"!eslint\"",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "new-version": "changeset",
    "publish-pkg": "changeset publish",
    "publish-pkg-alpha": "pnpm publish --tag alpha --no-git-checks --loglevel verbose",
    "release-alpha": "cross-env NODE_ENV=production pnpm build & pnpm publish-pkg-alpha",
    "release": "cross-env NODE_ENV=production pnpm build & pnpm publish-pkg",
    "diff": "pnpm outdated && pnpm diff",
    "deploy-netlify-sb-preview": "cross-env NODE_OPTIONS=--max-old-space-size=16384 pnpm build && pnpm build-sb && netlify deploy --dir=storybook-static/sb --site=4b420380-aed1-4dc6-b002-6efe7b413025 --open",
    "serve-sb": "serve storybook-static/sb",
    "serve-sb-chroma": "serve storybook-static/chromatic",
    "serve-sb-docs": "serve storybook-static/docs"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.9",
    "@chromatic-com/storybook": "3.2.2",
    "@hopper-ui/icons": "2.8.4",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.15",
    "@storybook/addon-a11y": "8.4.2",
    "@storybook/addon-essentials": "8.4.2",
    "@storybook/addon-interactions": "8.4.2",
    "@storybook/addon-links": "8.4.2",
    "@storybook/addon-mdx-gfm": "8.4.2",
    "@storybook/addon-webpack5-compiler-swc": "1.0.5",
    "@storybook/blocks": "8.4.2",
    "@storybook/react": "8.4.2",
    "@storybook/react-webpack5": "8.4.2",
    "@storybook/test": "8.4.2",
    "@storybook/test-runner": "0.19.1",
    "@storybook/types": "8.4.2",
    "@svgr/core": "6.5.1",
    "@swc/core": "1.7.36",
    "@swc/helpers": "0.5.13",
    "@swc/jest": "0.2.36",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.1",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.13",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@types/react-is": "18.3.0",
    "@types/resize-observer-browser": "0.1.11",
    "@typescript-eslint/parser": "8.10.0",
    "@workleap/eslint-plugin": "3.2.3",
    "@workleap/stylelint-configs": "2.0.3",
    "@workleap/swc-configs": "2.2.3",
    "@workleap/tsup-configs": "3.0.6",
    "@workleap/typescript-configs": "3.0.2",
    "browserslist": "4.24.0",
    "chalk": "4.1.2",
    "chromatic": "6.3.4",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "csstype": "*",
    "date-fns": "2.29.3",
    "eslint": "8.57.1",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.3.0",
    "formik": "2.4.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "meow": "9.0.0",
    "netlify-cli": "17.37.2",
    "prettier-local": "npm:prettier@2.5.1",
    "prism-react-renderer": "1.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-test-renderer": "18.3.1",
    "resize-observer-polyfill": "1.5.1",
    "serve": "14.2.4",
    "shelljs": "0.8.5",
    "storybook": "8.4.2",
    "stylelint": "^16.10.0",
    "svgo": "3.3.2",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "tsup": "8.3.5",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.11.0"
}
