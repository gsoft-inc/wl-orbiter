    },
    "author": "Groupe Sharegate inc.",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "style": "dist/index.css",
    "files": [
        "/dist",
        "!/dist/**/docs/*",
        "!/dist/**/tests/*",
