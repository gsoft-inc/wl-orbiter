{
    "name": "@orbit-ui/transition-components",
    "type": "module",
    "version": "5.4.0",
    "description": "Orbiter components",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-orbiter.git",
