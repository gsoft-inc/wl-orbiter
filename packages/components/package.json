    },
    "devDependencies": {
        "@hopper-ui/components": "^1.3.19",
        "@hopper-ui/styled-system": "^2.4.0",
        "@popperjs/core": "2.11.2",
        "react-aria-components": "^1.2.1",
        "text-mask-core": "5.1.2",
        "tsup": "^8.2.4",
