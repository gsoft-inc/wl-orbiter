{
    "name": "@workleap/orbiter-ui",
    "version": "5.6.2",
    "description": "Orbiter design system",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-orbiter.git",
        "directory": "packages/bundle"
    },
    "type": "module",
    "sideEffects": [
        "*.css"
    ],
    "author": "Groupe ShareGate inc.",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "style": "dist/index.css",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./index.css": "./dist/index.css"
    },
    "files": [
        "/dist",
        "!/dist/**/docs/*",
        "!/dist/**/tests/*",
        "index.css"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "scripts": {
        "prebuild": "pnpm clean",
        "build": "tsup --config ./tsup.build.ts",
        "clean": "pnpm dlx rimraf dist"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*",
        "@orbit-ui/transition-components": "5.6.2",
        "@orbit-ui/transition-css": "2.4.1",
        "@hopper-ui/components": "*",
        "@hopper-ui/styled-system": "*",
        "react-aria-components": "*"
    },
    "devDependencies": {
        "@orbit-ui/transition-components": "workspace:*",
        "@orbit-ui/transition-css": "workspace:*"
    }
}
